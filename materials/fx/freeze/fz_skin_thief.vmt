"vertexlitgeneric"
{


	"$basetexture" "FX/Magicreflection/MR_add_"
	"$model" 1
	"$SEPARATEALPHATEXTURE" "FX/Freeze/FZ_skin_warrior"

	

	"$bumpmap" "FX/Freeze/FZ_skin_warrior_normal"

	"$texscale" 	"[1 1]"
	"$translate"	1


	"$normalmapalphaenvmapmask" 1
	"$envmapcontrast" 0

	"$additive" 1

	"$translucent" 1

	"$envmapsaturation" 1.5
	

	"$envmaptint" "[1 1 1]"
	"$selfillum" 1

	"$envmap" "FX/Freeze/env_iceglove"

	"vertexlitgeneric_HDR_dx9"
		{
			"$envmap" "FX/Freeze/env_iceglove_hdr_"
			"$envmaptint" "[.6 .6 .6]"
		
		"Proxies"
			{
		"Sine"
		{
			"resultVar" "$selfillumtint[0]"
			"sineperiod" 3
			"sinemin" .8

			"sinemax" 1
		}
		"Sine"
		{
			"resultVar" "$selfillumtint[1]"
			"sineperiod" 3
			"sinemin" 0.6

			"sinemax" 1
		}
		"Sine"
		{
			"resultVar" "$selfillumtint[2]"
			"sineperiod" 3
			"sinemin" 0.6

			"sinemax" 1
		}

			"sine"
				{
					"sineperiod" .37
					"sinemin" 1
					"sinemax" 1
					"resultvar" "$alpha"
				}
			"AnimatedTexture"
	
				{
					"animatedtexturevar" "$basetexture"
					"animatedtextureframenumvar" "$frame"
					"animatedtextureframerate" 25.00
				}


			}
		
		
		
		
		}

	
	"Proxies"
	{


	"sine"
		{
			"sineperiod" .37
			"sinemin" .95
			"sinemax" 1
			"resultvar" "$alpha"
		}
	"AnimatedTexture"
	
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 25.00
		}


	}

}