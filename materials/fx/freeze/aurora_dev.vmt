 "Unlittwotexture"
{
	"$basetexture" "FX/Freeze/aurora_diffuse"
	"$texture2" "FX/Freeze/aurora_rays2"

	"$translucent" "1"
	"$additive" 1

	"$nodecal" 1

	//"$envmap" "env_cubemap"
	//"$envmaptint" "[1 1 1]"

	"$gnoise" "1"

	"$playerdistance" "0"
	"$playerdistance2" "1"

	"$alpharesult" "1"
	"$alpharesultmin" "1"
	"$alpharesultmax" "1"

	"$smallamount" ".02"
	"$largeamount" "1"

	"$hundred" "100"
	"$ten" "22"
	
	"$frameminusten" "1"

"Proxies"
	{

		"PlayerProximity"
		{
			"resultVar" "$playerdistance2"
			"scale"  .1
		}	
		"Subtract"
		{
			"srcVar1" "$playerdistance2"
			"srcVar2" "$ten"
			"resultVar" "$frameminusten"
		}
		"Clamp"
		{
			"srcVar1"	"$frameminusten"
			"min"		"0"
			"max"		"30"
			"resultVar" "$frame2"
		}
		"Clamp"
		{
			"srcVar1"	"$frameminusten"
			"min"		"0"
			"max"		"30"
			"resultVar" "$frame"
		}

		"PlayerProximity"
		{
			"resultVar" "$playerdistance"
			"scale" 0.001 
		}	
		"GaussianNoise"
		{
			"minVal" 0
			"maxVal" 1	
 			"mean" .7
			"halfwidth" 0.1
			"resultVar" "$gnoise"
		}
		"Subtract"
		{
			"srcVar1" "$gnoise"
			"srcVar2" "$playerdistance"
			"resultVar" "$alpharesultmin"
		}
		"Multiply"
		{
			"srcVar1" "$hundred"
			"srcVar2" "$playerdistance"
			"resultVar" "$largeamount"
		}
		"Equals"
		{
			"srcVar1" "$playerdistance"
			"resultVar" "$envmaptint[1]"
		}	
		"Add"
		{
			"srcVar1" "$alpharesultmin"
			"srcVar2" "$smallamount"
			"resultVar" "$alpharesultmax"
		}	
		"Sine"
		{
			
			"sineperiod"	.5

			"sinemin"	"$alpharesultmin"
			"sinemax"	"$alpharesultmax"
			"resultVar"	"$alpha"
		}


		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" .02
			"texturescrollangle" 0
		}

		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" 0
			"texturescrollangle" 180
		}
	}
}