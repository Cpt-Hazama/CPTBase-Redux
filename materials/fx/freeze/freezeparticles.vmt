 "Unlittwotexture"
{
	"$basetexture" "FX/Freeze/comball"
	"$texture2" "FX/Freeze/freeze_particle_mask"

	//"$translucent" 1
	"$additive" 1

	"$vertexalpha" 1
	"$vertexcolor" 1
	
	"$nodecal" 1




	"Proxies"
	{

		"UniformNoise"
		{
			"minVal" .85
			"maxVal" 1	
 			//"mean" .5
			//"halfwidth" .5
			"resultVar" "$alpha"

		}


		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" 0
			"texturescrollangle" 90
		}

		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" 60
			"texturescrollangle" 45
		}
		
		"AnimatedOffsetTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 25.00
		}
		

	}
}