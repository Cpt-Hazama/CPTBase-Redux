"unlitgeneric"
{
	"$basetexture" "FX/Freeze/ray_core01" 
	"$model" 1
	//"$additive" 1


	"$texscale" 1

	"$translucent" 1
	"$nocull" 1
	"$texoffset" "[0 0]"

	"$nodecal" 1

	"Proxies"

	{

		"Sine"
		{
			"resultVar" "$texscale"
			"sineperiod" .05
			"sinemin" .9
			"sinemax" 1
		}
	


		"Texturetransform"
	
		{
			"translatevar" "$texoffset"
			"scalevar" "$texscale"
			"resultvar" "$basetexturetransform"
		}



	}









}