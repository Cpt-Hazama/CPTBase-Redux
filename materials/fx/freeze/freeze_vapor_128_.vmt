
//"Sprite"


"Unlitgeneric"
{
	"$spriteorientation" "vp_parallel_oriented"
	"$spriteorigin" "[ 0.50 0.50 ]"
	"$spritescale" ".2"
	"$translucent" 1
	"$basetexture" "FX/Freeze/Freeze_vapor_128_"

	"$vertexcolor" 1
	"$vertexalpha" 1

	//"$additive" 1

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 200
		}

			"Sine"
		{
			
			"sineperiod"	10

			"sinemin"	.2
			"sinemax"	.8
			"resultVar"	"$alpha"
		}

		"Sine"

		{
			"sineperiod"	10
			"resultVar" "$color[0]"
			"sineperiod" .1
			"sinemin" .4
			"sinemax" .5
		}
		
		"Sine"
		{
			"sineperiod"	10
			"resultVar" "$color[1]"
			"sineperiod" .1
			"sinemin" .8
			"sinemax" .9
		}
		
		"Sine"
		{
			"sineperiod"	10
			"resultVar" "$color[2]"
			"sineperiod" .1
			"sinemin" .9
			"sinemax" 1
		}


	}
}
