

"Unlitgeneric"
{
	
	"$spriteorientation"	"4"
	"$spriteorigin" "[ 0.50 0.50 ]"

		
	"$model"	"1"
	"$basetexture" "FX/Souldrinker/SD_purple_haze_"
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"$translucent" "1"
	"$saturation" 1
	"$additive" 1
	
	"Proxies"
	{
		"Sine"
		{
			"resultVar" "$alpha"
			"sineperiod" 3
			"sinemin" .3

			"sinemax" .5
		}
	
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 25
		}



	}
}








